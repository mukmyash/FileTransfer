{
  "FTPHostedService": {
    "HOST": "192.168.1.1",
    "Port": 21,
    "ScanPerion": 10,
    "PathScaning": "/",
    "DownloadFolder": "./tmp",
    "Steps": [
      {
        "Type": "CheckDublicate",
        "Settings": {
          "PathStore": "./worked-files-store"
        }
      },
      {
        "Type": "TransforFileName",
        "Settings": {
          "FileMask": "BPS_@{FP1}"
        }
      },
      {
        "Type": "TransforXSLContent",
        "Settings": {
          "XSLTPath": "./xslt/transform.xslt"
        }
      }
    ]
  }
}
